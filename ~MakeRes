# Resource definition file(s)set SOURCE_DIR		'Beyond the Blue:Source Code:MacVax CC:'set SOURCE_FILE		'VaxCC_Frontend.r'# Rez outputset OUTPUT_DIR		'Beyond the Blue:Source Code:MacVax CC:'set OUTPUT_FILE		'VaxCC..rsrc'set FILE_TYPE		'rsrc'set FILE_CREATOR	'RSED'# Include directories.set INCLUDE_DIR		'Beyond the Blue:Source Code:SM/MW:Distribution:Rez:'# Other Rez optionsset REZ_OPTIONS		'-a -ov'			# Append and overwrite resources on output.# Do the Rez...rez     {REZ_OPTIONS}							   "{SOURCE_DIR}{SOURCE_FILE}"				-o "{OUTPUT_DIR}{OUTPUT_FILE}"				-i "{INCLUDE_DIR}"							-t "{FILE_TYPE}"							-c "{FILE_CREATOR}"